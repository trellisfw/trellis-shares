{
  "search.exclude": {
    "tags.*": true,
    ".dccache": true,
    ".history": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
