  "eslint.nodePath": ".yarn/sdks",
  "files.exclude": {
    "tags.*": true,
    ".dccache": true,
    ".history": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true
  },
  "eslint.packageManager": "yarn",
  "eslint.useESLintClass": true,
  "licenser.author": "Qlever LLC",
  "licenser.license": "AL2",
  "cSpell.words": [
    "Buildx",
    "codeql",
    "cois",
